  "dependencies": {
    "axios": "^0.18.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-confirm-alert": "^2.4.1",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-spinner-material": "^1.1.1",
    "react-text-truncate": "^0.14.1",
    "redux": "^4.0.5",
    "redux-form": "^8.2.1",
    "redux-promise": "^0.6.0",
    "redux-react-hook": "^3.3.2",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.5",
    "uikit": "^3.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --fix --ext .js,.jsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "test": "react-scripts test --passWithNoTests",
    "fix-js": "eslint-auto-fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "prettier": {
    "*.+(json|css|md|js|jsx)": "prettier --write --fix --ext .js,.jsx ."
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix --ext .js,.jsx .",
      "git add"
    ],
    "*.+(json|css|md|js|jsx)": [
      "prettier --write --fix --ext .js,.jsx .",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-auto-fix": "^1.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0"
  }
